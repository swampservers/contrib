"VertexLitGeneric"
{
	"$basetexture" "models/swamponions/vape/glass"
	"$translucent" 1
	"$surfaceprop" "glass"
	"$nocull" 1
	
	"$envmap" "env_cubemap"
	"$envmapcontrast" 0
	"$envmapsaturation" 1
	"$selfillum" 0
	"$model" 1

	$envmaptintbase "[ 1 1 1 ]"

	Proxies
	{
		VapeTankColor
		{
			resultVar	$color2
		}
		Multiply
		{
			srcVar1 $color2
			srcVar2 $envmaptintbase
			resultVar	$envmaptint
		}
	}
}
